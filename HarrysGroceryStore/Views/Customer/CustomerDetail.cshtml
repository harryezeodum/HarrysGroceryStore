@model Customer

<label class="font-weight-bold"> Customer Id: </label> @Model.CustomerId
<br />
<label class="font-weight-bold"> User Id: </label> @Model.UserId
<br />
<label class="font-weight-bold"> First Name: </label> @Model.FirstName
<br />
<label class="font-weight-bold"> Last Name: </label> @Model.LastName
<br />
<label class="font-weight-bold"> Phone Number: </label> @Model.PhoneNumber
<br />
<label class="font-weight-bold"> Address: </label> @Model.Address
<br />
<label class="font-weight-bold"> City: </label> @Model.City
<br />
<label class="font-weight-bold"> State: </label> @Model.State
<br />
<label class="font-weight-bold"> ZipCode: </label> @Model.ZipCode
<br />
<hr />


<a asp-controller="Customer" asp-action="UpdateCustomer" asp-route-id="@Model.CustomerId" class="btn btn-dark"> Edit Customer </a>
<a asp-controller="Customer" asp-action="DeleteCustomer" class="btn btn-dark" asp-route-id="@Model.CustomerId"> Delete Customer </a>
<a asp-controller="Customer" asp-action="Index" asp-route-id="@Model.CustomerId" class="btn btn-dark"> Back </a>
